{
  "expo": {
    "name": "Keyce-Afridine",
    "slug": "testafridine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logofinal.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/logofinal.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logofinal.jpg",
        "backgroundColor": "#0F172A"
      },
      "package": "com.eliemakoda.KeyceAfridine"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logofinal.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3be9c397-fc71-4591-b179-a280e74229a3"
      }
    },
    "owner": "eliemakoda"
  }
}
